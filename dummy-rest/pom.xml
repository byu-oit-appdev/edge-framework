	<profiles>
		<profile>
			<id>genkey</id>
			<activation>
				<file>
					<missing>${user.home}/localhost.jks</missing>
				</file>
			</activation>
			<properties>
				<keypass>jetty7</keypass>
				<storepass>${keypass}</storepass>
				<keyAlias>jetty7</keyAlias>
				<keystoreLocation>${project.build.outputDirectory}/jetty-ssl.keystore</keystoreLocation>
			</properties>
			<build>
				<plugins>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>keytool-maven-plugin</artifactId>
						<version>1.0</version>
						<executions>
							<execution>
								<phase>generate-resources</phase>
								<id>clean</id>
								<goals>
									<goal>clean</goal>
								</goals>
							</execution>
							<execution>
								<phase>generate-resources</phase>
								<id>genKey</id>
								<goals>
									<goal>genkey</goal>
								</goals>
							</execution>
						</executions>
						<configuration>
							<keystore>${keystoreLocation}</keystore>
							<dname>cn=localhost.byu.edu</dname>
							<keypass>${keypass}</keypass>
							<storepass>${storepass}</storepass>
							<alias>${keyAlias}</alias>
							<keyalg>RSA</keyalg>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<id>localhostKeystore</id>
			<activation>
				<file>
					<exists>${user.home}/localhost.jks</exists>
				</file>
			</activation>
			<properties>
				<keypass>tomcat</keypass>
				<storepass>${keypass}</storepass>
				<keyAlias>localhost_byu_edu</keyAlias>
				<keystoreLocation>${user.home}/localhost.jks</keystoreLocation>
			</properties>
		</profile>
	</profiles>

