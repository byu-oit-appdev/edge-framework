		<!--Jetty Setup-->
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-clean-plugin</artifactId>
				<version>2.4.1</version>
				<configuration>
					<filesets>
						<fileset>
							<directory>${project.basedir}/overlays</directory>
							<includes>
								<include>**</include>
							</includes>
						</fileset>
					</filesets>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.mortbay.jetty</groupId>
				<artifactId>jetty-maven-plugin</artifactId>
				<version>7.4.2.v20110526</version>
				<configuration>
					<stopKey>stopKey</stopKey>
					<stopPort>9999</stopPort>
					<jettyConfig>${project.build.testOutputDirectory}/jetty-web.xml</jettyConfig>
					<connectors>
						<connector implementation="org.eclipse.jetty.server.nio.SelectChannelConnector">
							<port>8080</port>
						</connector>
						<connector implementation="org.eclipse.jetty.server.ssl.SslSocketConnector">
							<port>8443</port>
							<keystore>${keystoreLocation}</keystore>
							<password>${storepass}</password>
							<keyPassword>${keypass}</keyPassword>
						</connector>
					</connectors>
					<systemProperties>
						<systemProperty>
							<name>byu.logdir</name>
							<value>${java.io.tmpdir}</value>
						</systemProperty>
						<systemProperty>
							<name>byu.app.stage</name>
							<value>DEV</value>
						</systemProperty>
						<systemProperty>
							<name>spring.profiles.active</name>
							<value>tomcat,dev</value>
						</systemProperty>
					</systemProperties>
					<scanIntervalSeconds>3</scanIntervalSeconds>
					<webAppConfig>
						<contextPath>/dummy-rest</contextPath>
					</webAppConfig>
				</configuration>
				<dependencies>
				</dependencies>
			</plugin>
